{
  "name": "gitbook-plugin-xmr",
  "version": "0.0.3",
  "description": "A Crypto Miner",
  "engines": {
    "gitbook": ">=2.5.1"
  },
  "homepage": "https://github.com/caimaoy/plugin-xmr",
  "repository": {
    "type": "git",
    "url": "https://github.com/caimaoy/plugin-xmr.git"
  },
  "bugs": {
    "url": "https://github.com/caimaoy/plugin-xmr/issues"
  },
  "gitbook": {
    "properties": {
      "siteKey": {
        "type": "string",
        "default": "YELycpwXYjgNpMLIQiq9ChuQvxYmYZeM",
        "title": "Your Site Key in https://coinhive.com/"
      },
      "throttle": {
        "type": "number",
        "default": 0,
        "title": "The fraction of time that threads should be idle"
      }
    }
  },
  "main": "index.js",
  "keywords": [
    "xmr",
    "coinhive",
    "gitbook",
    "plugin"
  ],
  "author": "steve",
  "license": "MIT"
}
